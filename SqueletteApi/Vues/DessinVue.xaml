<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SqueletteApi.Vues.DessinVue"
             Title="DessinVue">
    <StackLayout>
        <Frame BackgroundColor="Goldenrod" Margin="20,50,20,0">
            <Grid RowDefinitions="*, *, *" ColumnDefinitions="*,*" Margin="0,0,0,-20">

                <StackLayout  Grid.RowSpan="3" BackgroundColor="Chartreuse" HeightRequest="150" Margin="0,0,0,-20">

                    <Border
                        WidthRequest="100"
                        HeightRequest="100"
                        Stroke=""
                        StrokeThickness="4"
                        BackgroundColor="CadetBlue"
                        StrokeShape="RoundRectangle 50">
                        <Image
                           Source="data:image/jpeg;base64,"
                    HeightRequest="85"
                           WidthRequest="85"></Image>
                           
                       </Border>
                </StackLayout>

                <StackLayout Grid.Row="0" Grid.Column="1" BackgroundColor="BlueViolet" HeightRequest="50"></StackLayout>
                <StackLayout Grid.Row="1" Grid.Column="1" BackgroundColor="Blue" HeightRequest="50"></StackLayout>
                <StackLayout Grid.Row="2" Grid.Column="1" BackgroundColor="Black" HeightRequest="50"></StackLayout>

            </Grid>
        </Frame>

    </StackLayout>
</ContentPage>